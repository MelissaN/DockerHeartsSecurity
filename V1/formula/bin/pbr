#!/home/blockness-site/mit/formula/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pbr==4.0.2','console_scripts','pbr'
__requires__ = 'pbr==4.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pbr==4.0.2', 'console_scripts', 'pbr')()
    )
